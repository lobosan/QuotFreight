{
  "name": "quotfreight",
  "author": "Santiago Galindo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "build": "next build",
    "dev": "concurrently \"pnpm run dev:next\" \"pnpm run dev:convex\"",
    "dev:convex": "pnpm exec convex dev",
    "dev:next": "next dev --turbopack",
    "format": "prettier --write .",
    "lint": "next lint",
    "setup": "pnpm install && npx tsx scripts/setup.ts",
    "start": "next start"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@radix-ui/themes": "^3.2.1",
    "@workos-inc/authkit-nextjs": "^2.4.4",
    "@workos-inc/node": "^7.61.0",
    "@workos-inc/widgets": "1.2.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.0",
    "convex": "^1.25.4",
    "convex-helpers": "^0.1.99",
    "jose": "^6.0.12",
    "lucide-react": "^0.525.0",
    "next": "15.4.3",
    "next-themes": "^0.4.6",
    "nextjs-toploader": "^3.8.16",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "stripe": "^18.3.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "chalk": "^5.4.1",
    "eslint": "^9.31.0",
    "eslint-config-next": "15.4.3",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.8.3"
  }
}
